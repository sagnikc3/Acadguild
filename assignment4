
public class assignment4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x=5,y=10;
		
		int a= x + y * 2;
		int b= x - y + 2;
		int c= (x+y) * 2;
		int d= y % x;
		System.out.println(" Output 1:"+a+"\n Output 2:"+b+"\n output 3:"+c+"\n output 4:"+d );
		

	}

}
