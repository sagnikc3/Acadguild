
public class assignment2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		System.out.println("The Prime Numbers are :");
		System.out.print("1,2");
		
		for(int i=3;i<=100;i++)
		{
			int p=i;
				for(int j=2;j<i;j++)
				{
					p=i%j;
					if(p==0)
						break;
				}
				if(p!=0){
				System.out.print("," +i);
				}

	}
	}
}
